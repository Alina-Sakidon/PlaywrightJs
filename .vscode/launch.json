{
    "version": "0.2.0",
    "configurations": [
        
      {
        "type": "node",
        "name": "Debug Playwright Test",
        "request": "launch",
        "program": "${workspaceFolder}/node_modules/@playwright/test/cli.js",
        "args": ["test"],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "cwd": "${workspaceFolder}",
        "env": {
          "PWDEBUG": "1"
        }
      }
    ]
  }
  